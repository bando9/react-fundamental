</div>\n\n        {todos.length > 0 ? (\n          <div className=\"todos\">\n            {todos.map((todo, index, arr) => {\n              return (\n                <div key={index} className={`todo ${!(arr.length === index + 1) && 'todo-divider'}`}>\n\n\n                  {todo.title}\n                  <div className=\"todo-icon-wrapper\">\n                    <div className=\"todo-count\">{todo.count}</div>\n\n                    <button onClick={() => handleSubtraction(index)} className=\"todo-action-button\">\n                      <img src={minusIcon} alt=\"minus icon\"/>\n                    </button>\n\n                    <button onClick={() => handleAdditionCount(index)} className=\"todo-action-button\">\n                      <img src={plusIcon} alt=\"plus icon\"/>\n                    </button>\n                  </div>\n                </div>\n              )\n            })}\n          </div>\n        ) : (\n          <div>Kosong</div>\n        )}\n      </Container>\n    </>\n  );\n}\n\nexport default App;\n","D:\\ando\\repo github\\react fundamental\\starter\\counter-starter\\src\\components\\navbar\\index.js",[],"D:\\ando\\repo github\\react fundamental\\starter\\counter-starter\\src\\components\\container\\index.js",[],"D:\\ando\\repo github\\react fundamental\\starter\\counter-starter\\src\\components\\searchInput\\index.js",["27"],{"ruleId":"28","replacedBy":"29"},{"ruleId":"30","replacedBy":"31"},{"ruleId":"32","severity":1,"message":"33","line":2,"column":8,"nodeType":"34","messageId":"35","endLine":2,"endColumn":18},{"ruleId":"32","severity":1,"message":"36","line":1,"column":21,"nodeType":"34","messageId":"35","endLine":1,"endColumn":25},"no-native-reassign",["37"],"no-negated-in-lhs",["38"],"no-unused-vars","'classnames' is defined but never used.","Identifier","unusedVar","'func' is defined but never used.","no-global-assign","no-unsafe-negation"]